<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coswick.travelinktrial">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.Promo"/>

        <activity android:name=".activity.Notifikasi" />

        <activity android:name=".wisata.WisataWonogiri"
            android:theme="@style/WisataYogya" />

        <activity android:name=".wisata.WisataSurabaya"
            android:theme="@style/WisataYogya" />

        <activity android:name=".wisata.WisataMedan"
            android:theme="@style/WisataYogya" />

        <activity android:name=".wisata.WisataBali"
            android:theme="@style/WisataYogya" />

        <activity android:name=".wisata.WisataAceh"
            android:theme="@style/WisataYogya" />

        <activity android:name=".wisata.WisataKlaten"
            android:theme="@style/WisataYogya" />

        <activity android:name=".wisata.WisataSemua"
            android:theme="@style/WisataYogya" />

        <activity
            android:name=".wisata.WisataYogya"
            android:theme="@style/WisataYogya" />

        <activity android:name=".activity.DetailWisataPopuler" />

        <activity android:name=".activity.DetailWIsata" />

        <activity android:name=".activity.FormPembelian" />

        <activity android:name=".activity.BottomNav" />

        <activity android:name=".activity.Register" />

        <activity android:name=".activity.LogIn" />

        <activity android:name=".activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>